    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch main",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/src/example_1/main",
            "args": [
                "1.5",
                "3.0",
                "300",
                "200",
                "350",
                "550"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}